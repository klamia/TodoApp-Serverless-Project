{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todo",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+(\\s[a-zA-Z0-9]+)*$"
    },
    "dueDate": {
      "type": "string",
      "pattern": "^\\d{4}\\-(0?[1-9]|1[012])\\-(0?[1-9]|[12][0-9]|3[01])$"
    }
  },
  "required": [
    "name",
    "dueDate"
  ],
  "additionalProperties": false
}
